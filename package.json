{
  "name": "nucleusplatform",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "email:start": "export EMAIL_ASSET_ROOT_URL=/email && webpack-dev-server --config scripts/emails/webpack.config.js --hot --env development",
